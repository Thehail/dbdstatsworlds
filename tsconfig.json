{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ESNext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "outDir": "./dist",
        "rootDir": ".",
        "resolveJsonModule": true
    },
    "include": [
        "server/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}